{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true
  },
  "globals": {
  },
  "ecmaFeatures": {
    "jsx": true,
    "modules": true
  },
  "parser": "babel-eslint",
  "rules": {
    "camelcase": 2,
    "curly": 2,
    "brace-style": [
      2,
      "1tbs"
    ],
    "quotes": [
      2,
      "single"
    ],
    "semi": [
      1,
      "always"
    ],
    "eqeqeq": 1,
    "no-redeclare": 2,
    "key-spacing": [
      2,
      {
        "beforeColon": false,
        "afterColon": true
      }
    ],
    "no-constant-condition": 2,
    "react/display-name": 1
  }
}